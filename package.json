{
  "name": "codenames",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "buildStrap": {
    "nodeVersion": "20.18.0",
    "yarnVersion": "1.22.22",
    "repoType": "git",
    "copyright": "murrayju",
    "releaseBranch": "main",
    "devBranch": "dev",
    "npm": {
      "publish": false
    },
    "docker": {
      "repository": "murrayju/codenames",
      "registry": "ghcr.io"
    }
